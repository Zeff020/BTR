	keywords = {1107002176,Analytic Mechanics (Mathematical Aspects),Cambridge University Press,Computer Science,Computer Science (Specific Aspects),Computers - General Information,Computers / Computer Science,Isaac L. Chuang,Michael A. Nielsen,Physics,Physics - Quantum Theory,Quantum Computation and Quantum Information: 10th,Quantum computers,Quantum physics (quantum mechanics {\&} quantum field,Science,Science / Physics / Quantum Theory,Science/Mathematics},
	pmid = {11928608},
