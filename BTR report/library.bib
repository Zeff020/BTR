@article{Shor2000,
	abstract = {We prove the security of the 1984 protocol of Bennett and Brassard (BB84) for quantum key distribution. We first give a key distribution protocol based on entanglement purification, which can be proven secure using methods from Lo and Chau's proof of security for a similar protocol. We then show that the security of this protocol implies the security of BB84. The entanglement-purification based protocol uses Calderbank-Shor-Steane (CSS) codes, and properties of these codes are used to remove the use of quantum computation from the Lo-Chau protocol.},
	author = {Shor, Peter W. and Preskill, John},
	doi = {10.1103/PhysRevLett.85.441},
	issn = {00319007},
	journal = {Physical Review Letters},
	title = {{Simple proof of security of the BB84 quantum key distribution protocol}},
	year = {2000}
}

